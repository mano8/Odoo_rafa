#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/receipt_screen.js:0
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Print Individual Receipts"
msgstr "Imprimir Recibos Individuales"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "This order contains prepaid products."
msgstr "Este pedido contiene productos prepagados."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Each prepaid product will be printed separately."
msgstr "Cada producto prepagado se imprimirá por separado."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Do not lose your tickets; to claim the products, you need to exchange them at the bar."
msgstr "No pierda sus tickets; para reclamar los productos, deberá canjearlos en la barra."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "List of Prepaid Products:"
msgstr "Lista de Productos Prepagados:"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "The product above has been prepaid."
msgstr "El producto anterior ha sido prepagado."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Please exchange this ticket for your product at the bar."
msgstr "Por favor, canjee este ticket por su producto en la barra."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Customer Receipt"
msgstr "Recibo del Cliente"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Cashier Receipt"
msgstr "Recibo del Cajero"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Prepaid Product"
msgstr "Producto Prepagado"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Options"
msgstr "Opciones"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Cancel"
msgstr "Cancelar"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Select Products for Individual Receipt Printing"
msgstr "Seleccionar Productos para Impresión de Recibos Individuales"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Select all products that need to be printed separately."
msgstr "Seleccione todos los productos que necesiten ser impresos por separado."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "For each selected product, depending on the quantity defined, a ticket will be printed for each unit of the same product."
msgstr "Para cada producto seleccionado, dependiendo de la cantidad definida, se imprimirá un ticket por cada unidad del mismo producto."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "No items found in the order."
msgstr "No se encontraron artículos en el pedido."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Select All"
msgstr "Seleccionar Todo"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Deselect All"
msgstr "Deseleccionar Todo"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Print Full Cashier Receipt"
msgstr "Imprimir Recibo Completo del Cajero"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Print Full Customer Receipt"
msgstr "Imprimir Recibo Completo del Cliente"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Print Detailed Individual Receipts"
msgstr "Imprimir Recibos Individuales Detallados"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Quantity"
msgstr "Cantidad"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Unit Price"
msgstr "Precio Unitario"