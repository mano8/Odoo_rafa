#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/receipt_screen.js:0
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Print Individual Receipts"
msgstr "Imprimer les reçus individuels"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "This order contains prepaid products."
msgstr "Cette commande contient des produits prépayés."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Each prepaid product will be printed separately."
msgstr "Chaque produit prépayé sera imprimé séparément."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Do not lose your tickets; to claim the products, you need to exchange them at the bar."
msgstr "Ne perdez pas vos tickets; pour réclamer les produits, vous devrez les échanger au bar."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "List of Prepaid Products:"
msgstr "Liste des produits prépayés :"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "The product above has been prepaid."
msgstr "Le produit ci-dessus a été prépayé."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Please exchange this ticket for your product at the bar."
msgstr "Veuillez échanger ce ticket contre votre produit au bar."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Customer Receipt"
msgstr "Reçu client"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Cashier Receipt"
msgstr "Reçu caissier"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/pos_order.js:0
msgid "Prepaid Product"
msgstr "Produit prépayé"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Options"
msgstr "Options"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Cancel"
msgstr "Annuler"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Select Products for Individual Receipt Printing"
msgstr "Sélectionner les produits pour l'impression individuelle des reçus"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Select all products that need to be printed separately."
msgstr "Sélectionnez tous les produits qui doivent être imprimés séparément."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "For each selected product, depending on the quantity defined, a ticket will be printed for each unit of the same product."
msgstr "Pour chaque produit sélectionné, en fonction de la quantité définie, un ticket sera imprimé pour chaque unité du même produit."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "No items found in the order."
msgstr "Aucun article trouvé dans la commande."

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Select All"
msgstr "Sélectionner tout"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Deselect All"
msgstr "Désélectionner tout"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Print Full Cashier Receipt"
msgstr "Imprimer le reçu complet du caissier"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Print Full Customer Receipt"
msgstr "Imprimer le reçu complet du client"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Print Detailed Individual Receipts"
msgstr "Imprimer les reçus individuels détaillés"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Quantity"
msgstr "Quantité"

#. module: pos_indv_receipt
#. odoo-javascript
#: code:addons/pos_indv_receipt/static/src/overrides/components/models/product_selection_popup.js:0
msgid "Unit Price"
msgstr "Prix unitaire"