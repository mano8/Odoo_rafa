<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="pos_indv_receipt.ReceiptScreen" t-inherit="point_of_sale.ReceiptScreen" t-inherit-mode="extension">
        <xpath expr="//div[contains(@class, 'receipt-options')]" position="inside">
            <button class="button print btn btn-lg btn-secondary w-100 py-3" t-on-click="() => doOpenProductSelectionPopup.call()">
                <i class="fa fa-print"/> <t t-esc="labels.print_individual_receipt"/>
            </button>
        </xpath>
    </t>
</templates>